.container-fill{
	height: 100%;
	overflow: hidden;
}
.scroll-fullpage{
	height: 100%;
	transition: all 0.3s;
}


.section{
  height: 100%;
  background-image:url('data:image/jpeg;base64,');
}
.section-maintitle{
	display: block;
	font-size: 50rpx;
	color: rgb(18, 19, 87);
	font-weight: bold;
	letter-spacing: 10rpx;
	padding-top: 140rpx;
	padding-left: 40rpx;
}
.section-subtitle{
	padding-top: 30rpx;
	display: block;
	padding-left: 40rpx;
	font-size: 40rpx;
	color: rgb(18, 19, 87);
	font-weight: 700;
	letter-spacing: 10rpx;
}
.section-teammember{
	display: block;
	padding-top: 27rpx;
	padding-left: 40rpx;
	font-size: 32rpx;
	color: hsl(216, 58%, 49%);
	font-weight: 600;
	letter-spacing: 10rpx;
}
.join{	
	display: block;
  font-size: 45rpx;  
  font-weight: bold; 
  line-height: 50rpx;
  height: 85rpx;
  background-color: hsl(152, 85%, 35%);
  color: white;
	text-align: center;
	justify-content: center;
	border-radius: 40rpx; 
	margin-top: 82rpx;
	margin-left: 410rpx;
}
.active2 .section-maintitle,
.active2 .section-subtitle,
.active2 .section-teammember,
.active2 .join{
	animation: mymove 0.8s;
}
@keyframes mymove{
	from {
		transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
	}
	to {
		transform: translateY(0) scale(1) rotateY(0);
	}
}